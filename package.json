{
  "name": "treemotions",
  "displayName": "TreeMotions",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "treemotions.toggleMode",
        "title": "Toggle Mode"
      },
      {
        "command": "treemotions.navigateLeft",
        "title": "Navigate Left"
      },
      {
        "command": "treemotions.navigateDown",
        "title": "Navigate Down"
      },
      {
        "command": "treemotions.navigateUp",
        "title": "Navigate Up"
      },
      {
        "command": "treemotions.navigateRight",
        "title": "Navigate right"
      }
    ],
    "keybindings": [
      {
        "command": "treemotions.toggleMode",
        "key": "ctrl+alt+v",
        "when": "editorTextFocus"
      },
      {
        "command": "treemotions.navigateLeft",
        "key": "h",
        "when": "editorTextFocus && vimMode == 'command'"
      },
      {
        "command": "treemotions.navigateDown",
        "key": "j",
        "when": "editorTextFocus && vimMode == 'command'"
      },
      {
        "command": "treemotions.navigateUp",
        "key": "k",
        "when": "editorTextFocus && vimMode == 'command'"
      },
      {
        "command": "treemotions.navigateRight",
        "key": "l",
        "when": "editorTextFocus && vimMode == 'command'"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}